<div class="row">
  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <div class="row">
        <div class="input-field col s6">
          <%= f.email_field :email, autofocus: true, id: "email", type: "email", class: "validate" %>
          <label for="email">Email</label>
        </div>
    </div>
    
    <div class="row">
        <div class="input-field col s6">
          <%= f.password_field :password, autocomplete: "off", id: "password", type: "password" %>
          <label for="password">Password</label>
        </div>
    </div>
    <br></br>
    <div class="row">
      <div class="input-field col s6">
          <%= f.password_field :password_confirmation, autocomplete: "off", id: "password", type: "password" %>
          <label for="password">PasswordConfirmation</label>
      </div>
    </div>
    
    <div class="row">
      <div class="input-field col s6">
        <button class="btn waves-effect waves-light" type="submit" name="action">Submit
          <i class="material-icons right"></i>
        </button>
      </div>
    </div>
  <% end %>  
</div>

<%= devise_error_messages! %>

<%= render "devise/shared/links" %>